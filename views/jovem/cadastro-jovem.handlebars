<head>
  <!-- Custom fonts for this template-->
  <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
  <!-- Custom styles for this template-->
  <link href="/css/sb-admin-2.min.css" rel="stylesheet">
</head>
<body>

{{>dashboard}}
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content fluid">
        {{>dashboard-top}}
  
  <div class="container-fluid">

<!--Conteúdo da página ficará aqui-->


 <div class="container">

    <div class="card o-hidden border-0 shadow-lg my-5">
      <div class="card-body p-0">
        <!-- Nested Row within Card Body -->
        <div class="row">
          
          <div class="col-lg-12">
            <div class="p-4">
              <div class="text-center">


          <div class="card-header mb-5">
             
              <h6 class="m-0 font-weight-bold text-primary">Cadastro de Jovem</h6>
            </div>
                
               
              </div>
              <form action="#" method="POST" class="user">
 
                <div class="form-group">
                  <div class="row ">
                  <input type="text" name="nome" id="nome" class="form-control form-ajust30 grid-3 form-control-user" placeholder="Nome Completo*">
                  <input type="text" name="telefone" id="telefone" class="form-control form-ajust15 grid-2 form-control-user" placeholder="Telefone">
                   <input type="text" name="cmis" id="cmis" class="form-control form-ajust20 grid-2 form-control-user" placeholder="cmis">
                   <input type="text" name="ala" id="" class="form-control form-ajust20 grid-2 form-control-user" placeholder="Ala">
                
                  </div>
                </div>

                <div class="form-group mb-5">
                  <div class="row mb-2">
                     <label class="label grid-4">Idade*:</label>
                     <label class="label grid-4">Estaca:*</label>
                     <label class="label grid-8">Sexo*:</label>
                     <label class="label grid-5-2">Companhia:</label>

                  </div>
                  <div class="row">
                   
                  <input type="number" name="idade" id="idade" value="18" class="form-control form-ajust6 grid-3 form-control-user" placeholder="">
                  
                    <select class="select grid-3" name="estaca" id="estaca">
                        <option value="Estaca Salvador" selected>Estaca Salvador</option>
                        <option value="Estaca Salvador Norte">Estaca Salvador Norte</option>
                        <option value="Estaca Imbuí">Estaca Imbuí</option>
                        <option value="Estaca Liberdade">Estaca Liberdade</option>
                    </select>
              
                  <div class="grid-3 mt-3">
                       <input type="radio" name="sexo" value="M" checked="checked"> Masculino
                        <input type="radio" class="ml-2" name="sexo" value="F"> Feminino
                  </div>
               
                  

                  
                  <select name="companhia" class="select grid-3" required>
                  {{#each companhias}}
                  <option value="{{_id}}">{{nome}}</option>
                  {{else}}
                  <option value="0">Nenhuma categoria registrada</option>
                  {{/each}}
              </select>
                </div>
            </div>
            <button type="submit" id="" class=" btn btn-custom  btn-primary ">Cadastrar</button>
                
              </form>
              
            </div>
          </div>
        </div>
      </div>
    </div>

  </div>





<!--FIM do Conteúdo da página-->

        </div>    
      </div>
    </div>

{{>dashboard-bottom}}

  <!-- Bootstrap core JavaScript-->
  <script src="/js/jquery.min.js"></script>
  <script src="/js/bootstrap.bundle.min.js"></script>
  <!-- Core plugin JavaScript-->
  <script src="/js/jquery.easing.min.js"></script>
  <!-- Custom scripts for all pages-->
  <script src="/js/sb-admin-2.min.js"></script>
</body>

